<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qgwy.alpha_web_manager.mapper.UserMapper">

    <insert id="insertUserByBatch" parameterType="java.util.List" useGeneratedKeys="true">
        insert into user
        (username,age,gender,create_time)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (
                #{item.username},
                #{item.age},
                #{item.gender},
                #{item.createTime}
            )
        </foreach>
    </insert>
</mapper>